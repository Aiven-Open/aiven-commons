<?xml version="1.0" encoding="UTF-8"?>

<site xmlns="http://maven.apache.org/SITE/2.0.0"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://maven.apache.org/SITE/2.0.0 https://maven.apache.org/xsd/site-2.0.0.xsd"
      name="Aiven Common Utilities">


    <poweredBy>
        <logo name="Aiven" href="https://aiven.io/">
            <image src="https://aiven.io/remix-assets/logo-aiven-DLhBz3IO.svg"/>
        </logo>
    </poweredBy>
    <body>
        <menu ref="reports"/>

        <footer>Copyright &amp;copy Aiven Oy. Licensed under the Apache License, Version 2.0.
            Apache Commons Collection, Apache Kafka, and Apache Velocity are trademarks of The Apache Software Foundation.
            All other marks mentioned may be trademarks or registered trademarks of their respective owners.</footer>
    </body>
    <skin>
        <groupId>org.apache.maven.skins</groupId>
        <artifactId>maven-fluido-skin</artifactId>
        <version>2.0.1</version>
    </skin>
    <custom>
        <fluidoSkin>
            <topBarEnabled>false</topBarEnabled>
            <sideBarEnabled>true</sideBarEnabled>
            <skipGenerationDate>true</skipGenerationDate>
            <gitHub>
                <projectId>aiven-open/aiven-commons</projectId>
                <ribbonOrientation>right</ribbonOrientation>
                <ribbonColor>orange</ribbonColor>
            </gitHub>
        </fluidoSkin>
    </custom>
</site>